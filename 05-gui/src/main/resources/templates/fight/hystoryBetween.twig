{% extends "../default/base.twig" %}

{% block title %}Fight History with {{ name2 }}{% endblock %}

{% block row %}
    {% for fight in fights %}
        <a class="btn btn-lg btn-block btn-outline-danger col-sm-2" href="/fight/engage/{{ user }}/{{ name2 }}">New Fight</a>
        {% include "tableHistory.twig" with { details: fight.details} %}
    {% endfor %}
{% endblock %}